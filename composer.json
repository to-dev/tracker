{
  "name": "topoff/laravel-user-logger",
  "description": "Laravel User Logger",
  "keywords": ["laravel user logger"],
  "homepage": "https://github.com/topoff/laravel-user-logger",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "ndberg",
      "email": "a@bergerweb.ch"
    }
  ],
  "require": {
    "php": ">=7.0",
    "jaybizzle/crawler-detect": "~1.2",
    "jenssegers/agent": "~2.6",
    "piwik/device-detector": "^3.11",
    "laravel/framework": "~5.6",
    "ua-parser/uap-php": "~3.5",
    "snowplow/referer-parser": "~0.1",
    "topoff/user-agent-parser": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Topoff\\LaravelUserLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Topoff\\LaravelUserLogger\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Topoff\\LaravelUserLogger\\UserLoggerServiceProvider"
      ],
      "aliases": {
        "UserLogger": "Topoff\\LaravelUserLogger\\Facade"
      }
    }
  },
  "minimum-stability": "dev"
}
