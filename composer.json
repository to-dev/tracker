{
  "name": "topoff/tracker",
  "description": "Laravel Tracker",
  "keywords": [
    "tracker"
  ],
  "homepage": "https://github.com/topoff/tracker",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Berger",
      "email": "a@bergerweb.ch"
    }
  ],
  "require": {
    "php": ">=7.0",
    "jaybizzle/crawler-detect": "~1.2",
    "jenssegers/agent": "~2.6",
    "piwik/device-detector": "^3.9",
    "browscap/browscap-php": "^3.1",
    "laravel/framework": "~5.5",
    "ua-parser/uap-php": "~3.4",
    "snowplow/referer-parser": "~0.1",
    "thadafinser/user-agent-parser": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "repositories": [
    {
      "_comment": "Damit wird statt thadafinser/user-agent-parser das packet topoff/user-agent-parser verwendet, branch dev-master siehe oben. Erlaubt Bugfixes und weiterentwicklungen",
      "type": "vcs",
      "url": "https://github.com/topoff/user-agent-parser"
    }
  ],
  "autoload": {
    "psr-4": {
      "Topoff\\Tracker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Topoff\\Tracker\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Topoff\\Tracker\\TrackerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
