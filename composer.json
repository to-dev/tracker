{
  "name": "to-dev/tracker",
  "description": "Laravel Tracker",
  "keywords": [
    "tracker"
  ],
  "homepage": "https://github.com/to-dev/tracker",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Berger",
      "email": "a@bergerweb.ch"
    }
  ],
  "require": {
    "php": ">=5.3.7",
    "laravel/framework": "~5",
    "jenssegers/agent": "~2.1",
    "jaybizzle/crawler-detect": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "to-dev\\Tracker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "to-dev\\Tracker\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "to-dev\\Tracker\\TrackerServiceProvider"
      ],
      "aliases": {
        "Tracker": "to-dev\\Tracker\\Facade"
      }
    }
  },
  "minimum-stability": "dev"
}
